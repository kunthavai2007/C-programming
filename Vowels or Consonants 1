#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
  
         char alph;   
    scanf("%c",&alph);
    switch(alph){
            case 'a':
            case 'e':
            case 'i':
            case 'o':
            case 'u':
            case 'A':
            case 'E':
            case 'I':
            case 'O':
            case 'U':
            printf("The Character %c is Vowel",alph);
            break;
    default:
            printf("The Character %c is Consonant",alph);
    } 
    return 0;
}
