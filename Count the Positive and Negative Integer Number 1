#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
 int a,i,integer;
    int posi=0,neg=0;
    scanf("%d",&a);
    for(i=0;i<a;i++)
    {
        scanf("%d",&integer);
        if(integer>0)
        {
            posi++;
        }
        else if(integer<0)
        {
           neg++; 
        }
    }
         printf("Count of Positive Integer is %d.00\n",posi);
        printf("Count of Negative Integer is %d.00",neg);
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
